doctype html
html
  head
    title= title
    link(rel="stylesheet" href="/bower/bootstrap/dist/css/bootstrap.css")
    style.
      nav{
        background: #dec49b;
      }
    
  body
    nav.navbar.navbar-expand-lg.navbar-light.warning-color.lighten-5
      a.navbar-brand(href='#') Rotaract Club of VIT
      button.navbar-toggler(type='button', data-toggle='collapse', data-target='#navbarNav', aria-controls='navbarNav', aria-expanded='false', aria-label='Toggle navigation')
        span.navbar-toggler-icon
      #navbarNav.collapse.navbar-collapse
        ul.navbar-nav
          li.nav-item
            a.nav-link(href='../../') Home
          li.nav-item.active
            a.nav-link(href='../../#courses') Courses
          li.nav-item
            a.nav-link(href='#') About us
            
    .container-fluid.text-center
      .row.content
        .col-sm-2.sidenav   
          br
          br
          each c in course
            p
              a(href='#')= c.content
        .col-sm-8.text-left
          br
          br
          br
          h2= title
          br
          br
          p= description
          
          each c in course
            h3= c.content
            iframe(src="" +c.videos, width='560', height='315', frameborder='0', allowfullscreen='')
            br
            br
      
          hr
        .col-sm-2.sidenav
          br
          br
          .well
            p ADS
          .well
            p ADS

  
    br
    br
    hr
    script(src='/bower/jquery/dist/js/bootstrap.js')
    script(src='/bower/bootstrap/dist/js/bootstrap.js')  
