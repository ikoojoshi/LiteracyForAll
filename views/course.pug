doctype html
html
  head
    title= course.title
    link(rel="stylesheet" href="/bower/bootstrap/dist/css/bootstrap.css")
    style.
      nav{
        background: #dec49b;
      }
    
  body
    nav.navbar.navbar-expand-lg.navbar-light.warning-color.lighten-5
      a.navbar-brand(href='#') Rotaract Club of VIT
      button.navbar-toggler(type='button', data-toggle='collapse', data-target='#navbarNav', aria-controls='navbarNav', aria-expanded='false', aria-label='Toggle navigation')
        span.navbar-toggler-icon
      #navbarNav.collapse.navbar-collapse
        ul.navbar-nav
          li.nav-item
            a.nav-link(href='../../') Home
          li.nav-item.active
            a.nav-link(href='../../#courses') Courses
          li.nav-item
            a.nav-link(href='#') About us

    .container
      ///#courses
      h2= course.title
      p= course.description
      
      iframe(src='https://www.youtube.com/embed/PPTOfAAs7XQ', width='560', height='315', frameborder='0', allowfullscreen='')

              
    br
    br
    hr
    script(src='/bower/jquery/dist/js/bootstrap.js')
    script(src='/bower/bootstrap/dist/js/bootstrap.js')  
